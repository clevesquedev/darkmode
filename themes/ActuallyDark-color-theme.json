{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark High Contrast",
  "colors": {
    "editor.background": "#090909",
    "editor.foreground": "#ffffffc4",
    "editorIndentGuide.background": "#ffffff79",
    "editorIndentGuide.activeBackground": "#faf9f918",
    "editor.selectionBackground": "#faf9f946",
    "editor.selectionHighlightBackground": "#faf9f917",
    "editor.wordHighlightBackground": "#faf9f932",
    "sideBarTitle.foreground": "#fdfdfdca",
    "sideBar.background": "#090909",
    "sideBar.foreground": "#ffffffc4",
    "sideBar.border": "#00bb006b",
    "sideBySideEditor.horizontalBorder": "#00bb006b",
    "sideBySideEditor.verticalBorder": "#00bb006b",
    "tree.indentGuidesStroke": "#ffffffc5",
    "panel.border": "#0080009b",
    "sideBarSectionHeader.background": "#00bb006b",
    "selection.background": "#00bb006b",
    "statusBar.background": "#1a1a1a",
    "activityBar.activeBackground": "#00bb006b",
    "activityBar.foreground": "#ffffffca",
    "activityBar.background": "#1a1a1a",
    "breadcrumb.background": "#1a1a1a",
    "contrastActiveBorder": "#00bb006b",
    "dropdown.background": "#1a1a1a",
    "foreground": "#ffffffca",
    "menubar.selectionBackground": "#00bb006b",
    "tab.hoverBackground": "#00bb006b",
    "tab.inactiveBackground": "#141414",
    "tab.border": "#00bb006b",
    "tab.activeBorder": "#0af60a6b",
    "tab.lastPinnedBorder": "#00bb006b",
    "tab.activeBorderTop": "#00bb006b",
    "titleBar.activeBackground": "#0a0a0a",
    "titleBar.activeForeground": "#FFFFFFca",
    "terminal.border": "#00bb006b",
    "terminal.foreground": "#bebebe",
    "terminal.tab.activeBorder": "#FFFFFFca",
    "activityBarBadge.background": "#0080009b",
    "ports.iconRunningProcessForeground": "#FFFFFFca",
    "editorWhitespace.foreground": "#7c7c7c"
  },
  "tokenColors": [
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": { "foreground": "#FFFFFFca" }
    },
    { "scope": "emphasis", "settings": { "fontStyle": "italic" } },
    { "scope": "strong", "settings": { "fontStyle": "bold" } },
    { "scope": "meta.diff.header", "settings": { "foreground": "#000080f5" } },
    { "scope": "comment", "settings": { "foreground": "#7ca668" } },
    { "scope": "constant.language", "settings": { "foreground": "#569cd6f5" } },
    {
      "scope": [
        "constant.numeric",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": { "foreground": "#b5cea8f5" }
    },
    { "scope": "constant.regexp", "settings": { "foreground": "#b46695f5" } },
    {
      "scope": "constant.character",
      "settings": { "foreground": "#569cd6f5" }
    },
    { "scope": "entity.name.tag", "settings": { "foreground": "#569cd6f5" } },
    {
      "scope": "entity.name.tag.css",
      "settings": { "foreground": "#d7ba7df5" }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#9cdcfef5" }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.scss"
      ],
      "settings": { "foreground": "#d7ba7df5" }
    },
    { "scope": "invalid", "settings": { "foreground": "#f44747f5" } },
    { "scope": "markup.underline", "settings": { "fontStyle": "underline" } },
    { "scope": "markup.bold", "settings": { "fontStyle": "bold" } },
    {
      "scope": "markup.heading",
      "settings": { "fontStyle": "bold", "foreground": "#6796e6f5" }
    },
    { "scope": "markup.italic", "settings": { "fontStyle": "italic" } },
    {
      "scope": "markup.strikethrough",
      "settings": { "fontStyle": "strikethrough" }
    },
    { "scope": "markup.inserted", "settings": { "foreground": "#b5cea8f5" } },
    { "scope": "markup.deleted", "settings": { "foreground": "#ce9178f5" } },
    { "scope": "markup.changed", "settings": { "foreground": "#569cd6f5" } },
    {
      "name": "brackets of XML/HTML tags",
      "scope": ["punctuation.definition.tag"],
      "settings": { "foreground": "#808080f5" }
    },
    { "scope": "meta.preprocessor", "settings": { "foreground": "#569cd6f5" } },
    {
      "scope": "meta.preprocessor.string",
      "settings": { "foreground": "#ce9178f5" }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": { "foreground": "#b5cea8f5" }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": { "foreground": "#9cdcfef5" }
    },
    { "scope": "storage", "settings": { "foreground": "#569cd6f5" } },
    { "scope": "storage.type", "settings": { "foreground": "#569cd6f5" } },
    { "scope": "storage.modifier", "settings": { "foreground": "#569cd6" } },
    { "scope": "string", "settings": { "foreground": "#ce9178" } },
    { "scope": "string.tag", "settings": { "foreground": "#ce9178" } },
    { "scope": "string.value", "settings": { "foreground": "#ce9178" } },
    { "scope": "string.regexp", "settings": { "foreground": "#d16969" } },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": { "foreground": "#569cd6" }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": { "foreground": "#ffffffab" }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": { "foreground": "#d4d4d4" }
    },
    { "scope": "keyword", "settings": { "foreground": "#569cd6f5" } },
    { "scope": "keyword.control", "settings": { "foreground": "#569cd6f5" } },
    { "scope": "keyword.operator", "settings": { "foreground": "#d4d4d4f5" } },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.logical.python"
      ],
      "settings": { "foreground": "#569cd6f5" }
    },
    {
      "scope": "keyword.other.unit",
      "settings": { "foreground": "#b5cea8f5" }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": { "foreground": "#d4d4d4f5" }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": { "foreground": "#b5cea8f5" }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": { "foreground": "#d4d4d4ab" }
    },
    {
      "name": "coloring of the TS this",
      "scope": "variable.language.this",
      "settings": { "foreground": "#569cd6f5" }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member"
      ],
      "settings": { "foreground": "#DCDCAAf5" }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "entity.name.class",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": { "foreground": "#4EC9B0f5" }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class"
      ],
      "settings": { "foreground": "#4EC9B0f5" }
    },
    {
      "name": "Control flow / Special keywords",
      "scope": [
        "keyword.control",
        "source.cpp keyword.operator.new",
        "source.cpp keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.operator"
      ],
      "settings": { "foreground": "#c586c0f5" }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": { "foreground": "#9cdcfef5" }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": ["meta.object-literal.key"],
      "settings": { "foreground": "#9CDCFEf5" }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": { "foreground": "#CE9178f5" }
    },
    {
      "name": "HC Search Editor context line override",
      "scope": "meta.resultLinePrefix.contextLinePrefix.search",
      "settings": { "foreground": "#CBEDCBf5" }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "newOperator": "#FFFFFF",
    "stringLiteral": "#ce9178",
    "customLiteral": "#DCDCAA",
    "numberLiteral": "#b5cea8"
  }
}
